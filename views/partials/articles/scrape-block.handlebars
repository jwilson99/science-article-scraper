<div class="card articleDiv">
    <div class="card-header">
        <p class="card-title">
            {{title}}
        </p>
    </div>

    <div class="card-block">
        <p class="card-text">
            <a href="http://www.sciencemag.org{{link}}" target="_blank">Read the article!</a>
        </p>
    </div>
    <button class="save-button btn btn-primary" data-title="{{this.title}}" data-link="{{link}}">
        SAVE!
    </button>
</div>